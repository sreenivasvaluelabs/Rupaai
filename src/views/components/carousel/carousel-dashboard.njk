 {% set carouselItems = [{title:"", description: "", image: "img/Presentation-Components@2x.jpg" },
 {title:"", description: "", image: "img/Live-editor-banner-3@2x.jpg"}, 
 {title:"", description: "", image: "img/Download-banner-2@2x.jpg"}, 
 {title:"", description: "", image: "img/Custom-Download-banner-2@2x.jpg"}, 
 {title:"", description: "", image: "img/Unit-testing@2x.jpg"}]%}
 
 <div class="container">
    <div class="row">
        <div class="col-md-12">
      <div id="dashboard-carousel" class="carousel slide carousel-fade" data-ride="carousel">
              <ol class="carousel-indicators">
                <li data-target="#dashboard-carousel" data-slide-to="0" class="active"></li>
                <li data-target="#dashboard-carousel" data-slide-to="1"></li>
                <li data-target="#dashboard-carousel" data-slide-to="2"></li>
                <li data-target="#dashboard-carousel" data-slide-to="3"></li>
                <li data-target="#dashboard-carousel" data-slide-to="4"></li>
              </ol>
              <div class="carousel-inner" role="listbox">
              {%for item in carouselItems %}
                {%if loop.first== true %}
                <div class="carousel-item active">
                {%else%}
                <div class="carousel-item">
                {%endif%}
                  <div class="view">
                    <img class="d-block w-100" src= '{{item.image}}' alt="slide-img">
                    <div class="mask rgba-black-light"></div>
                  </div>
                  <div class="carousel-caption">
                    <h3 class="h3-responsive">{{item.title}}</h3>
                    <p>{{item.description}}</p>
                  </div>
                </div>
                {%endfor%}
              </div>        
            </div>
    
        </div>
      </div>
    </div>