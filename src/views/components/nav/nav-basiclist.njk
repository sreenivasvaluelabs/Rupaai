{% set list=[{title: "Home"}, {title: "About"},{title: "Services"}, {title: "Personal Info"}] %}
{% block content %}
<div class="container">
    <div class="row">
        <div class="col-md-12 custom-nav-list">
            <ul class="nav">
                {% for item in list%}
                <li class="nav-item">
                {%if loop.last == true%}
                <a class="nav-link disabled" href="#">{{item.title}}</a>
                {%else%}
                <a class="nav-link" href="#">{{item.title}}</a>
                {%endif%}
                </li>
                {%endfor%}                
            </ul>
        </div>
    </div>
</div>
{% endblock %}